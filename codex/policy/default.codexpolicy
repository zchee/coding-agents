prefix_rule(
    pattern = ["git"],
    decision = "allow",
    match = [
        ["git", "add"],
        ["git", "commit"],
        ["git", "log"],
        ["git", "status"],
    ],
)

prefix_rule(
    pattern = ["/usr/bin/git"],
    decision = "allow",
    match = [
        ["/usr/bin/git", "add"],
        ["/usr/bin/git", "commit"],
        ["/usr/bin/git", "log"],
        ["/usr/bin/git", "status"],
    ],
)

prefix_rule(
    pattern = ["execshell"],
    decision = "allow",
    match = [
        ["execshell"],
    ],
)
